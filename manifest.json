{

  "manifest_version": 2,
  "name": "JiraLink",
  "version": "1.0",

  "description": "Adds a red border to all webpages matching mozilla.org.",

  "icons": {
    "256": "icons/jira.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["JiraLink.js"]
    }
  ]

}